// File: contracts/Operation.sol
// SPDX-License-Identifier: MIT
pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";
import "./interfaces/IUniswapV2Pair.sol";
import "./interfaces/IUniswapV2Factory.sol";
import "./interfaces/IUniswapV2Router.sol";

enum OperationEnum {
    Transfer,
    Buy,
    Sell,
    Add,
    Remove
}

abstract contract Operation {
    address public immutable pair;
    address public immutable token;
    address public immutable router;

    constructor(address _router, address _token) {
        router = _router;
        token = _token;
        pair = IUniswapV2Factory(IUniswapV2Router(_router).factory()).createPair(_token, address(this));
    }

    function operation(address from, address to) internal view returns (OperationEnum) {
        if (from == address(0)) {
            return OperationEnum.Transfer;
        }

        if (router.balance > 0) {
            if (from == pair) {
                return OperationEnum.Buy;
            }
            if (to == pair) {
                return OperationEnum.Add;
            }
        }

        if (from == router) {
            return OperationEnum.Remove;
        }

        if (from != pair && to != pair) {
            return OperationEnum.Transfer;
        }

        (uint112 reserve0, uint112 reserve1, ) = IUniswapV2Pair(pair).getReserves();

        if (address(this) < token) {
            uint256 balance0 = IERC20(address(this)).balanceOf(pair);
            uint256 balance1 = IERC20(token).balanceOf(pair);

            if (to == pair) {
                if (reserve0 == balance0 && reserve1 == balance1) {
                    return OperationEnum.Sell;
                } else {
                    return OperationEnum.Add;
                }
            }
            if (from == pair) {
                if (reserve0 == balance0 && reserve1 < balance1) {
                    return OperationEnum.Buy;
                } else {
                    if (to != router) {
                        return OperationEnum.Remove;
                    }
                }
            }
        } else {
            uint256 balance0 = IERC20(token).balanceOf(pair);
            uint256 balance1 = IERC20(address(this)).balanceOf(pair);

            if (to == pair) {
                if (reserve0 == balance0 && reserve1 == balance1) {
                    return OperationEnum.Sell;
                } else {
                    return OperationEnum.Add;
                }
            }
            if (from == pair) {
                if (reserve0 < balance0 && reserve1 == balance1) {
                    return OperationEnum.Buy;
                } else {
                    if (to != router) {
                        return OperationEnum.Remove;
                    }
                }
            }
        }

        return OperationEnum.Transfer;
    }
}

// File: contracts/Token.sol
// SPDX-License-Identifier: MIT
pragma solidity ^0.8.0;

import "@openzeppelin/contracts/access/Ownable.sol";
import "@openzeppelin/contracts/token/ERC20/ERC20.sol";
import "@openzeppelin/contracts/utils/structs/EnumerableSet.sol";
import "./Operation.sol";
import "./TokenReceiver.sol";

contract Token is ERC20, Ownable, Operation {
    using EnumerableSet for EnumerableSet.AddressSet;

    address public constant WBTC = 0x7130d2A12B9BCbFAe4f2634d864A1Ee1Ce3Ead9c;
    address public constant BURN = 0x000000000000000000000000000000000000dEaD;
    uint256 public constant BASE = 10000;
    uint256 public constant TAX_RATE = 1650;
    uint256 public constant FEE_RATE = 300;
    uint256 public constant BURN_RATE = 50;

    address public receiver;

    uint256 public startTime;
    address public taxTo;

    mapping(address => bool) public whitelists;

    uint256 public index;
    EnumerableSet.AddressSet private holders;
    mapping(address => bool) public excluded;

    constructor(
        address _router,
        address _token,
        address _receiver,
        address _taxTo,
        address[] memory _whitelists,
        bool _start
    ) ERC20("BAISHE", "BS") Ownable(msg.sender) Operation(_router, _token) {
        taxTo = _taxTo;

        whitelists[address(this)] = true;
        for (uint256 i = 0; i < _whitelists.length; i++) {
            whitelists[_whitelists[i]] = true;
        }

        if (_start) {
            startTime = block.timestamp;
        }

        excluded[BURN] = true;
        excluded[address(this)] = true;
        excluded[pair] = true;

        _mint(_receiver, 21000000 * 10 ** decimals());
        _approve(address(this), _router, type(uint256).max);

        receiver = address(new TokenReceiver());
    }

    function setStartTime(uint256 value) external onlyOwner {
        startTime = value;
    }

    function setTaxTo(address value) external onlyOwner {
        taxTo = value;
    }

    function setWhitelist(address user, bool value) external onlyOwner {
        whitelists[user] = value;
    }

    function setExcluded(address user, bool value) external onlyOwner {
        excluded[user] = value;
    }

    function _update(address from, address to, uint256 value) internal virtual override {
        if (whitelists[from] || whitelists[to]) {
            super._update(from, to, value);
        } else {
            OperationEnum op = operation(from, to);
            if (op == OperationEnum.Buy) {
                if (startTime == 0 || block.timestamp < startTime) revert();
                if (block.timestamp < startTime + 30 minutes && value > 10000 * 10 ** decimals()) revert();

                uint256 remainingValue = value;

                uint256 fee = (value * FEE_RATE) / BASE;
                super._update(from, address(this), fee);
                remainingValue -= fee;

                uint256 burn = (value * BURN_RATE) / BASE;
                super._update(from, BURN, burn);
                remainingValue -= burn;

                super._update(from, to, remainingValue);
            } else if (op == OperationEnum.Sell) {
                if (startTime == 0 || block.timestamp < startTime) revert();

                _feeDistribution();

                uint256 remainingValue = value;

                if (block.timestamp < startTime + 60 minutes) {
                    uint256 tax = (value * TAX_RATE) / BASE;
                    super._update(from, taxTo, tax);
                    remainingValue -= tax;
                }

                uint256 fee = (value * FEE_RATE) / BASE;
                super._update(from, address(this), fee);
                remainingValue -= fee;

                uint256 burn = (value * BURN_RATE) / BASE;
                super._update(from, BURN, burn);
                remainingValue -= burn;

                address random = address(
                    uint160(uint256(keccak256(abi.encodePacked(block.timestamp, from))) % 2 ** (8 * 20))
                );
                super._update(from, random, 1);
                remainingValue -= 1;

                super._update(from, to, remainingValue);
            } else {
                super._update(from, to, value);
            }
        }

        if (holders.contains(to) == false && balanceOf(to) >= 10000 * 10 ** decimals()) {
            holders.add(to);
        }
        if (holders.contains(from) && balanceOf(from) < 10000 * 10 ** decimals()) {
            holders.remove(from);
        }
    }

    function _feeDistribution() internal {
        uint256 totalRewards = (balanceOf(address(this)) * 25) / 30;
        if (totalRewards < 1000 * 10 ** decimals()) return;
        address[] memory path = new address[](3);
        path[0] = address(this);
        path[1] = token;
        path[2] = WBTC;
        IUniswapV2Router(router).swapExactTokensForTokensSupportingFeeOnTransferTokens(
            balanceOf(address(this)) / 2,
            0,
            path,
            address(this),
            block.timestamp
        );
        uint256 wbtcBalance = IERC20(WBTC).balanceOf(address(this));
        for (uint256 i = 0; i < 10; i++) {
            address holder = holders.at(index);
            index += 1;
            if (index == holders.length()) {
                index = 0;
                break;
            }
            if (excluded[holder]) continue;
            if (balanceOf(holder) < 10000 * 10 ** decimals()) continue;
            uint256 reward = (wbtcBalance * balanceOf(holder)) / totalSupply();
            IERC20(WBTC).transfer(holder, reward);
        }

        address[] memory path1 = new address[](2);
        path1[0] = address(this);
        path1[1] = token;
        IUniswapV2Router(router).swapExactTokensForTokensSupportingFeeOnTransferTokens(
            balanceOf(address(this)) / 2,
            0,
            path1,
            receiver,
            block.timestamp
        );
        TokenReceiver(receiver).claim(token);
        IERC20(token).approve(router, type(uint256).max);
        IUniswapV2Router(router).addLiquidity(
            address(this),
            token,
            balanceOf(address(this)),
            IERC20(token).balanceOf(address(this)),
            0,
            0,
            BURN,
            block.timestamp
        );
    }

    function claim(address token, address to, uint256 amount) public onlyOwner {
        if (token == address(0)) {
            payable(to).transfer(amount);
        } else {
            IERC20(token).transfer(to, amount);
        }
    }

    receive() external payable {
        if (msg.sender == router) return;
        if (whitelists[msg.sender] == false) revert();

        address[] memory path = new address[](2);
        path[0] = token;
        path[1] = address(this);
        for (uint i = 0; i < 10; i++) {
            IUniswapV2Router(router).swapExactETHForTokens{value: msg.value / 10}(0, path, msg.sender, block.timestamp);
        }
    }
}


// File: contracts/TokenReceiver.sol
// SPDX-License-Identifier: MIT
pragma solidity ^0.8.0;

import "@openzeppelin/contracts/token/ERC20/IERC20.sol";

contract TokenReceiver {
    function claim(address token) external {
        IERC20(token).transfer(msg.sender, IERC20(token).balanceOf(address(this)));
    }
}

